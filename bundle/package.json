{
  "name": "@portal-web/directus-extension-portal-web-backend-bundle",
  "version": "1.0.0",
  "dependencies": {
    "@codexteam/icons": "^0.3.0",
    "@editorjs/attaches": "^1.3.0",
    "@editorjs/checklist": "^1.4.0",
    "@editorjs/code": "^2.8.0",
    "@editorjs/delimiter": "^1.3.0",
    "@editorjs/editorjs": "^2.26.5",
    "@editorjs/embed": "^2.5.3",
    "@editorjs/header": "^2.7.0",
    "@editorjs/image": "^2.8.1",
    "@editorjs/inline-code": "^1.4.0",
    "@editorjs/list": "^1.8.0",
    "@editorjs/marker": "^1.3.0",
    "@editorjs/nested-list": "^1.3.0",
    "@editorjs/paragraph": "^2.9.0",
    "@editorjs/personality": "^2.0.2",
    "@editorjs/quote": "^2.5.0",
    "@editorjs/raw": "^2.4.0",
    "@editorjs/simple-image": "^1.5.1",
    "@editorjs/table": "^2.2.1",
    "@editorjs/underline": "^1.1.0",
    "@editorjs/warning": "^1.3.0",
    "@sotaproject/strikethrough": "^1.0.1",
    "@types/node": "^18.15.3",
    "editorjs-alert": "^1.0.3",
    "editorjs-paragraph-with-alignment": "3.x",
    "editorjs-text-alignment-blocktune": "^1.0.3",
    "editorjs-undo": "^2.0.20",
    "lodash": "^4.17.21"
  },
  "directus:extension": {
    "host": "^10.0.0",
    "type": "bundle",
    "path": {
      "app": "dist/app.js",
      "api": "dist/api.js"
    },
    "entries": [
      {
        "type": "hook",
        "name": "hooks",
        "source": "src/hooks/index.ts"
      },
      {
        "type": "interface",
        "name": "interfaces/content-blocks",
        "source": "src/interfaces/content-blocks/index.ts"
      }
    ]
  },
  "description": "Portal web backend app extensions bundle",
  "icon": "extension",
  "keywords": [
    "directus",
    "directus-extension",
    "directus-custom-bundle"
  ],
  "files": [
    "dist",
    "!**/*.test.{d.ts,js}",
    "!**/*.d.ts.map"
  ],
  "scripts": {
    "build": "directus-extension build",
    "dev": "directus-extension build -w --no-minify",
    "link": "directus-extension link",
    "add": "directus-extension add"
  },
  "devDependencies": {
    "@directus/extensions-sdk": "^10.1.4",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.4",
    "vue": "^3.2.47"
  }
}
